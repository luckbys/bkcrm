{
  "root": "dist/",
  "clean_urls": true,
  "https_only": true,
  "error_page": "index.html",
  "routes": {
    "/**": "index.html"
  },
  "headers": {
    "/": {
      "Cache-Control": "no-cache, no-store, must-revalidate"
    },
    "/static/**": {
      "Cache-Control": "public, max-age=31536000"
    },
    "/assets/**": {
      "Cache-Control": "public, max-age=31536000"
    }
  }
} 